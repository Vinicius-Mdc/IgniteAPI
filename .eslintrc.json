{
  "env": {
    "es2021": true,
    "commonjs": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier"],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off"
  }
}
